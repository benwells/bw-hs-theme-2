# HubSpot Configuration
# This file is auto-generated by hs init
# Add it to .gitignore to keep credentials secure

defaultPortal: YOUR_PORTAL_ID
portals:
  - name: production
    portalId: YOUR_PORTAL_ID
    authType: personalaccesskey
    personalAccessKey: YOUR_ACCESS_KEY
    auth:
      tokenInfo:
        accessToken: YOUR_ACCESS_KEY
